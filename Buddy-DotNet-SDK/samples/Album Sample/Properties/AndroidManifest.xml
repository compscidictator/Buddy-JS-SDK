<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.buddy.AlbumsSample">
	<uses-sdk />
	<application android:label="Albums Sample">
		<activity android:name="createloginuserlibrary.LoginActivity" android:exported="true">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity android:name="albumssample.AlbumsActivity" android:exported="true">
			<intent-filter>
				<action android:name="createloginuserlibrary.intent.action.MAIN"/>
				<category android:name="android.intent.category.DEFAULT"/>
			</intent-filter>
		</activity>
	</application>
</manifest>