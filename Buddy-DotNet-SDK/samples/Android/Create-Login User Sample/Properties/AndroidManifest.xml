<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.buddy.CreateLoginUserSample">
	<uses-sdk />
	<application android:label="Create-Login User Sample" android:theme="@style/DefaultTheme">
		<activity android:name="createloginuserlibrary.LoginActivity" android:exported="true">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity android:name="createloginusersample.ViewUserActivity" android:exported="true">
			<intent-filter>
				<action android:name="createloginuserlibrary.intent.action.MAIN"/>
				<category android:name="android.intent.category.DEFAULT"/>
			</intent-filter>
		</activity>
	</application>
</manifest>